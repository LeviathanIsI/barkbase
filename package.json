{
  "name": "barkbase-react",
  "version": "1.0.0",
  "private": true,
  "description": "Data migration scripts and root dependencies.",
  "scripts": {
    "migrate:data": "node ./aws/scripts/migrate-data.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:workflows": "jest --testPathPattern=tests/workflows",
    "test:e2e": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e --runInBand",
    "test:e2e:auth": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/auth --runInBand",
    "test:e2e:entity": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/entity --runInBand",
    "test:e2e:operations": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/operations --runInBand",
    "test:e2e:financial": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/financial --runInBand",
    "test:e2e:analytics": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/analytics --runInBand",
    "test:e2e:config": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/config --runInBand",
    "test:e2e:messaging": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/messaging --runInBand",
    "test:e2e:integration": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e/integration --runInBand",
    "test:e2e:coverage": "cross-env E2E_TEST=true jest --testPathPattern=tests/e2e --runInBand --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.917.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@aws-sdk/client-scheduler": "^3.956.0",
    "@aws-sdk/client-sqs": "^3.956.0",
    "@playwright/test": "^1.57.0",
    "aws-sdk-client-mock": "^4.1.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0"
  }
}
